if (<%= @guest.coming %> == true) {
  //alert("<%=@guest.name%> is coming!")
  $('#can_<%= @guest.id %>').addClass( "selected" );
  $('#cant_<%= @guest.id %>').removeClass("selected");
} else {
  //alert("<%=@guest.name%> can't make it")
  $('#can_<%= @guest.id %>').removeClass("selected");
  $('#cant_<%= @guest.id %>').addClass( "selected" );;
}
